pipeline{
  agent any

  tools {
    maven 'M2_HOME'
    }

  stages {
     stage('Checkout') {
       steps {
         echo 'Checkout the source code from GitHub'
         git 'https://github.com/CJoyNazareth/star-agile-insurance-project.git'
             }
     }
     stage('Package') {
       steps {
         echo 'Create a Package'
         sh 'mvn package'
             }
     }
 }
 }
